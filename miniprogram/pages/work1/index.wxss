@import "./icon.wxss";
.body{
  width: 100%;
  height: 100vh;
  background-color: lightblue;
}
.com{
  width: 90%;
  height: 60px;
  background-color: rgba(0, 0, 0, .7);
  margin-top: 10px;
  border-radius: 8px;
  padding-left: 10px;
  color: #fff;
  margin: 10px auto;
  font-weight: bold;
  position: relative;
}
.iconfont{
  font-size: 30px;
  padding-right: 10px;
  vertical-align:middle;
}
.com span{
  line-height: 60px;
}
.sn{
  text-align: center;
  margin-top: 10px;
  margin-bottom: 10px;
  font-weight: bold;
  opacity: 0.5;
}
